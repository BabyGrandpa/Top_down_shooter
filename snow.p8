function make_snow()

 local snow_particle = {
 
 x=flr(rnd(200)),
 y=-2,
 sp=flr(rnd(4))+1,
 dx=-2,
 dy=3}
 add(snow,snow_particle) 
 --snowcounter+=1
end

function update_snow()

	make_snow()
 for a in all(snow) do
  a.x=a.x+a.dx
  a.y=a.y+a.dy
 end
 
end

function draw_snow()
	
	for a in all(snow) do
		spr(a.sp,a.x,a.y)
	end

end
