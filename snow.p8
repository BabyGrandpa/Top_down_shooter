-- colition

function c_snow()
 
 for a in all(snow) do
 	aimx=(a.x+a.dx)/8
		aimy=(a.y+a.dy)/8
	
		if	fget(mget(aimx,aimy),0) then
			del(snow,a)
	
		end
	end
end
