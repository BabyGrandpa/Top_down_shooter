function s()
	x=64
	y=64
end

function move()

	if btnp(⬆️) then
		y+=8
	end
	if btnp(⬇️) then
		y-=8
	end
	if btnp(⬅️) then
		x+=8
	end
	if btnp(➡️) then
		x-=8
	end

end



