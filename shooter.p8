function _init()
 poke(0x5f2d, 1)
 s()
 collide_map()
end



function _update()
	move()
	collide_map()
end


function _draw()
	cls()
	spr(39,64,64)
	spr(38,64,64)

	map(0,0,x,y,46,42)
end


