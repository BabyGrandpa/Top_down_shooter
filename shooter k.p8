function _init()
 poke(0x5f2d, 1)
 xmap=stat(32)
 ymap=stat(33)
 gameinit()
end



function _update()
	move()
	camera(cx,cy)
	mousef=stat(32)-1
	mousef2=stat(33)-1
	timer+=1
end


function _draw()
	cls()
	map(0,0,0,0,128,64)
	drawdoor()
	print(x.."/"..y,cx,cy,2)
	drawsht()
	spr(39,mousef+mx,mousef2+my)
end


