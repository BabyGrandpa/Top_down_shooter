-- arrays

function fire()
	local	b = {     
		sp = 37,
		x = playerx,
		y = playery,
	
		dx=3*m/l,	--dirx
	 dy=3*q/l --diry
	}
	
	add(bullets,b)
	
end

function bullet_update()

 q=(mousey+my-playery)    --delta y
 m=(mousex+mx-playerx) --delta x
 l=sqrt(q^2+m^2) -- length of dist(player;mouse)
 dx=3*m/l --dirx
 dy=3*q/l --diry

end

function swats()
 local s = {
 	sp = 35,
 	x = flr(rnd(9)),
 	y = flr(rnd(72)+80),
 	w = 8,
 	h = 8
 
	}

	add(swat,s)

end

function swat_fire()

	for s in all(swat) do
		local	sb = {     
			sp = 36,
			x = s.x,
			y = s.y
		}
	
		add(s_bullet,sb)
	end
	
end

function swat_go()


end
