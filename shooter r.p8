--
--
--



function game_init()

	playerx=64
	playery=64
	speedx=8
	speedy=8
	xc=64
	yc=64
	cx=0
	cy=0
	mx=0
	my=0
	doorv=0
	timer=0
	shrt=0
	fadet=0
	bullets = { }
	swat = { }
	s_bullet = { } 
	
end

function game_update()

 move()
 camera(cx,cy)
 mousex=stat(32)
	mousey=stat(33)
	bullet_update()
	cbullet()
	--swats()
 spawn_swat()
 go_swat()
 swat_shoot()
 
 for b in all(bullets) do
  b.x+=b.dx
  b.y+=b.dy
 end
 
 if stat(34) == 1  then
  fire() 
  shrt=0
  fadet=0
 end
 
end

function game_draw()
	cls()
 pal(13,128,1)
 pal(14,140,1)
 map(0,0,0,0,128,64)
 drawdoor()
 print(playerx.."/"..playery,cx,cy,7)
 print(stat(32).."/"..stat(33),cx,cy+8,7)
 spr(39,mousex+mx,mousey+my)
 line(playerx+4,playery+4,mousex+mx+4,mousey+my+4,7)
 print(timer,cx,cy+16,7)
 print(timer,cx,cy+24,7)
 drawbullets()
 drawswatbullet()
 

 drawswat()
 
 drawsht()
	

	
end



