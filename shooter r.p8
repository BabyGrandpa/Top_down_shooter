function drawdoor()
		
		if cdoorontop() == true then
			spr(1,playerx,playery)
			doorsound()
		end
	
end

function drawsht()
	if stat(32) < 64 then
		spr(38,playerx,playery,1,1,false,false)
	elseif stat(32) > 63 then
	 spr(38,playerx,playery,1,1,true,false)
	end
end 

function doorsound()
	if timer%10 == 0 then
		
		sfx(0)
	end
end

function drawswat()
	
	for s in all(swat) do
		spr(s.sp,s.x,s.y)
	end
	
end

function drawbullets()

	for b in all(bullets) do 
  spr(b.sp,b.x,b.y)
 end
 
end

function drawswatbullet()
	
	for sb in all(s_bullet) do
		spr(36,sb.x,sb.y)
	end
	
end
