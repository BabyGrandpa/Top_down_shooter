--timers

function spawn_swat()
	if timer%60 == 0 then
		swats()
	end
end

function go_swat()
	for s in all(swat) do
		swatx=(s.x+s.w)/8
		swaty=(s.y+(s.h)/2)/8
		if fget(mget(swatx,swaty),0) then
		else
			if timer%10 == 0 then
	 		s.x=s.x+1
	 	end
		end
	end
end

function swat_shoot()

		swat_fire()

end





